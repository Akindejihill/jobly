{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "additionalProperties": false,
  "required": [
      "title",
      "salary",
      "equity",
      "companyHandle"
  ],
  "properties": {
      "title": {
          "type": "string",
          "default": "",
          "title": "The title Schema",
          "examples": [
              "New"
          ]
      },
      "salary": {
          "type": "integer",
          "default": 0,
          "title": "The salary Schema",
          "examples": [
              240000
          ]
      },
      "equity": {
          "type": "number",
          "default": 0.0,
          "title": "The equity Schema",
          "examples": [
              0.1
          ]
      },
      "companyHandle": {
          "type": "string",
          "default": "",
          "title": "The companyHandle Schema",
          "examples": [
              "baker-santos"
          ]
      }
  },
  "examples": [{
      "title": "New",
      "salary": 240000,
      "equity": 0.1,
      "companyHandle": "baker-santos"
  }]
}